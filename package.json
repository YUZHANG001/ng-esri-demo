{
  "name": "ng-seed",
  "version": "1.0.0",
  "description": "angular seed app",
  "main": "src/index.html",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "ng": "ng",
    "build": "ng build --prod --base-href=/ng-seed/",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0 --base-href=/ng-seed/",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage",
    "e2e": "ng e2e --webdriver-update false",
    "webdriver-update": "webdriver-manager update --proxy http://127.0.0.1:10089"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beginor/ng-seed.git"
  },
  "keywords": [
    "angular",
    "seed",
    "angular-cli"
  ],
  "author": "beginor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/beginor/ng-seed/issues"
  },
  "homepage": "https://github.com/beginor/ng-seed#readme",
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "bootstrap": "^4.4.1",
    "@ng-bootstrap/ng-bootstrap": "^5.1.4",
    "@fortawesome/fontawesome-free": "^5.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular-devkit/build-ng-packagr": "~0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
